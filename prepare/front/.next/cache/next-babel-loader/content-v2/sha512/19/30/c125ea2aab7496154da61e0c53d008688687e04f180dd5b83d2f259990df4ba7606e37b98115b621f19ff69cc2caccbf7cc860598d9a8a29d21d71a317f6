{"ast":null,"code":"import produce from 'immer';\nexport const initialState = {\n  mainPosts: [],\n  addPostLoading: false,\n  addPostDone: false,\n  addPostError: null\n};\nexport const ADD_POST_REQUEST = 'ADD_POST_REQUEST';\nexport const ADD_POST_SUCCESS = 'ADD_POST_SUCCESS';\nexport const ADD_POST_FAILURE = 'ADD_POST_FAILURE';\n\nconst reducer = (state = initialState, action) => produce(state, draft => {\n  switch (action.type) {\n    case ADD_POST_REQUEST:\n      draft.addPostLoading = true;\n      draft.addPostDone = false;\n      draft.addPostError = null;\n      break;\n\n    case ADD_POST_SUCCESS:\n      draft.addPostLoading = false;\n      draft.addPostDone = true;\n      draft.mainPosts.unshift(action.data);\n      break;\n\n    case ADD_POST_FAILURE:\n      draft.addPostLoading = false;\n      draft.addPostError = action.error;\n      break;\n\n    default:\n      break;\n  }\n});\n\nexport default reducer;","map":{"version":3,"sources":["C:/Users/김승미/Desktop/for-portfolio/2021-April/prepare/front/reducers/post.js"],"names":["produce","initialState","mainPosts","addPostLoading","addPostDone","addPostError","ADD_POST_REQUEST","ADD_POST_SUCCESS","ADD_POST_FAILURE","reducer","state","action","draft","type","unshift","data","error"],"mappings":"AAAA,OAAOA,OAAP,MAAoB,OAApB;AAEA,OAAO,MAAMC,YAAY,GAAG;AAC1BC,EAAAA,SAAS,EAAE,EADe;AAE1BC,EAAAA,cAAc,EAAE,KAFU;AAG1BC,EAAAA,WAAW,EAAE,KAHa;AAI1BC,EAAAA,YAAY,EAAE;AAJY,CAArB;AAOP,OAAO,MAAMC,gBAAgB,GAAG,kBAAzB;AACP,OAAO,MAAMC,gBAAgB,GAAG,kBAAzB;AACP,OAAO,MAAMC,gBAAgB,GAAG,kBAAzB;;AAEP,MAAMC,OAAO,GAAG,CAACC,KAAK,GAAGT,YAAT,EAAuBU,MAAvB,KACdX,OAAO,CAACU,KAAD,EAASE,KAAD,IAAW;AACxB,UAAQD,MAAM,CAACE,IAAf;AACE,SAAKP,gBAAL;AACEM,MAAAA,KAAK,CAACT,cAAN,GAAuB,IAAvB;AACAS,MAAAA,KAAK,CAACR,WAAN,GAAoB,KAApB;AACAQ,MAAAA,KAAK,CAACP,YAAN,GAAqB,IAArB;AACA;;AACF,SAAKE,gBAAL;AACEK,MAAAA,KAAK,CAACT,cAAN,GAAuB,KAAvB;AACAS,MAAAA,KAAK,CAACR,WAAN,GAAoB,IAApB;AACAQ,MAAAA,KAAK,CAACV,SAAN,CAAgBY,OAAhB,CAAwBH,MAAM,CAACI,IAA/B;AACA;;AACF,SAAKP,gBAAL;