\n      dispatch({\n        type: ADD_POST_REQUEST,\n        data: {\n          title: title,\n          tag: currentHashtags,\n          expression: expressions\n        }\n      });\n    }\n  }, [title, currentHashtags, expressions]);\n  useEffect(() => {\n    if (addPostDone) {\n      Router.replace('/');\n    }\n  }, [addPostDone]);\n  return _jsxDEV(_Fragment, {\n    children: [_jsxDEV(Global, {}, void 0, false, {\n      fileName: _jsxFileName,\n      lineNumber: 161,\n      columnNumber: 7\n    }, this), _jsxDEV(_Fragment, {\n      children: [_jsxDEV(\"div\", {\n        css: writeBox,\n        children: [_jsxDEV(TextArea, {\n          css: titleBox,\n          placeholder: \"\\uC81C\\uBAA9\\uC744 \\uC785\\uB825\\uD574\\uC8FC\\uC138\\uC694\",\n          value: title,\n          onChange: onChangeTitle,\n          autoSize: {\n            minRows: 1\n          }\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 164,\n          columnNumber: 11\n        }, this), _jsxDEV(\"div\", {\n          css: tagBox,\n          children: [_jsxDEV(\"div\", {\n            css: tags,\n            children: [currentHashtags.map((v, index) => {\n              return _jsxDEV(\"button\", {\n                onClick: () => onClickTag(v),\n                children: v\n              }, v, false, {\n                fileName: _jsxFileName,\n                lineNumber: 175,\n                columnNumber: 19\n              }, this);\n            }), _jsxDEV(\"input\", {\n              placeholder: \"\\uD0DC\\uADF8\\uB97C \\uC785\\uB825\\uD574\\uC8FC\\uC138\\uC694\",\n              value: tag,\n              onChange: onChangeTag,\n              onKeyPress: onKeyPressTag,\n              onFocus: onFocusInput,\n              onBlur: onBlurInput\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 180,\n              columnNumber: 15\n            }, this)]\n          }, void 0, true, {\n            fileName: _j